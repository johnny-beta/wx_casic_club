<!--pages/index/detail/index.wxml-->
<view class="page">
  <view class="page__hd i-header">
      <view class="i-header-h1 h50">测试文字·1111</view>
    <view class="i-header-h1">测试文字ssssssssssssssssssss</view>
  </view>

    <view class="page__bd">
        <view class="weui-article">
            <view class="weui-article__h1"> {{rows.title}} {{rows.id}} {{rows.objectId}}</view>
            <view class="weui-article__section">
              

                <view class="weui-article__section">
                   
                    <view class="weui-article__p">
                      <text selectable='true'>{{rows.content}}</text>   
                    </view>
                     <view class="weui-article__h3">创建时间：{{rows.createdAt}} </view>
                </view>
            </view>
        </view>
    </view>
   <template is="foot"  />
</view>
